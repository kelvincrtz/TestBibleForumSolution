@model TestBibleForum.Models.Forum.ForumIndexModel

<h1>Forums</h1>

<table class="table table-hover">
    <tbody>
        @foreach (var forum in Model.ForumIndex)
        {
            <tr>
                <td>
                    @forum.Title
                </td>
                <td>
                    @forum.Description
                </td>
            </tr>
        }
    </tbody>
</table>